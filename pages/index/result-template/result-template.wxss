.dotting {
    display: inline-block;
    min-width: 4rpx;
    min-height: 4rpx;
    box-shadow: 4rpx 0 currentColor,12rpx 0 currentColor,20rpx 0 currentColor;
    animation: dot 2s infinite step-start both;
}

@-webkit-keyframes dot {
    25% {
        box-shadow: none;
    }

    50% {
        box-shadow: 4rpx 0 currentColor;
    }

    75% {
        box-shadow: 4rpx 0 currentColor,12rpx 0 currentColor;
    }
}

@keyframes dot {
    25% {
        box-shadow: none;
    }

    50% {
        box-shadow: 4rpx 0 currentColor;
    }

    75% {
        box-shadow: 4rpx 0 currentColor,12rpx 0 currentColor;
    }
}